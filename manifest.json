{
    "name": "Stock checker",
    "version": "0.0.1",
    "description": "stock Checker",

    "permissions":["<all_urls>","tabs",
    "activeTab"],
    "browser_action":{
        "default_popup":"popup.html",
        "default_icon":"logo1.png"
    },
   
    "content_scripts":[
        {"matches":["<all_urls>"],
        "js":["contentScript.js"],
        "run_at":"document_end"
        }
    ],
    "manifest_version":2,
    "icons":{
        "128":"logo1.png"
    }
}